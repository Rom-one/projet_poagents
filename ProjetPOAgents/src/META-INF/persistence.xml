<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="POAgentPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>data.Vente</class>
    <class>data.Objet</class>
    <class>data.Vendeur</class>
    <class>data.Stock</class>
    <class>data.Categorie</class>
    <class>data.Categorie_1</class>
    <class>data.Vendeur_1</class>
    <class>data.Vente_1</class>
    <class>data.Stock_1</class>
    <class>data.Objet_1</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/db_poagent"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.password" value="hosubi"/>
    </properties>
  </persistence-unit>
</persistence>
